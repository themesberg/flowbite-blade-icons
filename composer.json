{
    "name": "themesberg/flowbite-blade-icons",
    "description": "A package to easily make use of Flowbite Icons in your Laravel Blade views.",
    "keywords": ["Flowbite", "Blade", "Laravel"],
    "homepage": "https://github.com/themesberg/flowbite-blade-icons",
    "license": "MIT",
    "authors": [
        {
            "name": "Dominique Thomas",
            "email": "domthomas@ncoo-dev.com"
        }
    ],
    "require": {
        "php": "~8.1.0|~8.2.0|~8.3.0",
        "blade-ui-kit/blade-icons": "^1.6",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "pestphp/pest": "^1.23|^2.0",
        "phpunit/phpunit": "^9.0|^10.5|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Themesberg\\FlowbiteBladeIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Themesberg\\FlowbiteBladeIcons\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Themesberg\\FlowbiteBladeIcons\\FlowbiteBladeIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
